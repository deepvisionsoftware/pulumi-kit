{
  "name": "@deep/pulumi-kit",
  "version": "2.0.13",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\""
  },
  "main": "dist/index.js",
  "engines": {
    "node": "24"
  },
  "author": {
    "name": "DeepVision Code",
    "email": "code@deepvision.team"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cloudflare": {
      "import": "./dist/cloudflare/index.js",
      "require": "./dist/cloudflare/index.js",
      "types": "./dist/cloudflare/index.d.ts"
    },
    "./gcp": {
      "import": "./dist/gcp/index.js",
      "require": "./dist/gcp/index.js",
      "types": "./dist/gcp/index.d.ts"
    },
    "./github": {
      "import": "./dist/github/index.js",
      "require": "./dist/github/index.js",
      "types": "./dist/github/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "cloudflare": [
        "dist/cloudflare/index.d.ts"
      ],
      "gcp": [
        "dist/gcp/index.d.ts"
      ],
      "github": [
        "dist/github/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@pulumi/cloudflare": "^6.11.0",
    "@pulumi/gcp": "^8.41.1",
    "@pulumi/github": "^6.9.1",
    "@pulumi/pulumi": "^3.208.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  }
}
