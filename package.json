{
  "name": "@deep/pulumi-kit",
  "version": "1.1.9",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\""
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=20"
  },
  "author": {
    "name": "DeepVision Code",
    "email": "code@deepvision.team"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cloudflare": {
      "import": "./dist/cloudflare/index.js",
      "require": "./dist/cloudflare/index.js",
      "types": "./dist/cloudflare/index.d.ts"
    },
    "./gcp": {
      "import": "./dist/gcp/index.js",
      "require": "./dist/gcp/index.js",
      "types": "./dist/gcp/index.d.ts"
    },
    "./github": {
      "import": "./dist/github/index.js",
      "require": "./dist/github/index.js",
      "types": "./dist/github/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "cloudflare": [
        "dist/cloudflare/index.d.ts"
      ],
      "gcp": [
        "dist/gcp/index.d.ts"
      ],
      "github": [
        "dist/github/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@deep/text-kit": "^1.0.5",
    "@pulumi/cloudflare": "^5.25.0",
    "@pulumi/gcp": "^7.18.0",
    "@pulumi/github": "^6.2.0",
    "@pulumi/google-native": "^0.32.0",
    "@pulumi/pulumi": "^3.112.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@deep/eslint-plugin": "^1.1.30",
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.5"
  }
}
