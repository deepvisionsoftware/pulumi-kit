{
  "name": "@deep/pulumi-kit",
  "version": "2.0.5",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\""
  },
  "main": "dist/index.js",
  "engines": {
    "node": "22"
  },
  "author": {
    "name": "DeepVision Code",
    "email": "code@deepvision.team"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cloudflare": {
      "import": "./dist/cloudflare/index.js",
      "require": "./dist/cloudflare/index.js",
      "types": "./dist/cloudflare/index.d.ts"
    },
    "./gcp": {
      "import": "./dist/gcp/index.js",
      "require": "./dist/gcp/index.js",
      "types": "./dist/gcp/index.d.ts"
    },
    "./github": {
      "import": "./dist/github/index.js",
      "require": "./dist/github/index.js",
      "types": "./dist/github/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "cloudflare": [
        "dist/cloudflare/index.d.ts"
      ],
      "gcp": [
        "dist/gcp/index.d.ts"
      ],
      "github": [
        "dist/github/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@deep/text-kit": "^1.0.8",
    "@pulumi/cloudflare": "^6.2.0",
    "@pulumi/gcp": "^8.32.1",
    "@pulumi/github": "^6.7.2",
    "@pulumi/pulumi": "^3.171.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.24",
    "concurrently": "^9.1.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  }
}
