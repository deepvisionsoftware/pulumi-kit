{
  "name": "@deep/pulumi-kit",
  "version": "2.0.4",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\""
  },
  "main": "dist/index.js",
  "engines": {
    "node": "22"
  },
  "author": {
    "name": "DeepVision Code",
    "email": "code@deepvision.team"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cloudflare": {
      "import": "./dist/cloudflare/index.js",
      "require": "./dist/cloudflare/index.js",
      "types": "./dist/cloudflare/index.d.ts"
    },
    "./gcp": {
      "import": "./dist/gcp/index.js",
      "require": "./dist/gcp/index.js",
      "types": "./dist/gcp/index.d.ts"
    },
    "./github": {
      "import": "./dist/github/index.js",
      "require": "./dist/github/index.js",
      "types": "./dist/github/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "cloudflare": [
        "dist/cloudflare/index.d.ts"
      ],
      "gcp": [
        "dist/gcp/index.d.ts"
      ],
      "github": [
        "dist/github/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@deep/text-kit": "^1.0.7",
    "@pulumi/cloudflare": "^5.45.0",
    "@pulumi/gcp": "^8.12.0",
    "@pulumi/github": "^6.4.0",
    "@pulumi/pulumi": "^3.143.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@deep/eslint-plugin": "^1.1.33",
    "@types/node": "^20.17.10",
    "concurrently": "^9.1.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  }
}
